{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    }
  },
  "extensions": {
    "durableTask": {
      "hubName": "CaseGenHub",
      "storageProvider": { "type": "AzureStorage" },
      "tracing": {
         "distributedTracingEnabled": true,
         "version": "V2"
       }
    }
  },
  "functionTimeout": "00:30:00"
}